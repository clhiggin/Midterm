//Student Class//
using System;
using System.Collections.Generic;
using System.Text;

namespace Midterm_Q8
{
    class Student
    {
        public int Age { get; set; }

        public Student()
        {
            Age = 24;
        }
    public void Print()
        {
            Console.WriteLine("Age=" + Age );
        }
    }
}

//Program Class//
using System;

namespace Midterm_Q8
{
    class Program
    {
        static void Main(string[] args)
        {
            var student = new Student();
            student.Print();
            student.Age = 36;
            student.Print();
        }
      
    }
}

